<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_gravity="top"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:foregroundGravity="top"
        android:background="@drawable/estilo_menu_home"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">


        <com.google.android.material.button.MaterialButton
            android:id="@+id/btnHorariosDialog"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:backgroundTint="@color/white"
            android:text="Horarios"
            android:textAlignment="viewStart"
            android:textColor="@color/black"
            app:strokeWidth="0dp"
            android:textAllCaps="false"
            app:icon="@drawable/horarios"
            app:iconTint="@color/black"
            android:elevation="0dp"
            style="@style/Widget.AppCompat.Button.Borderless"
            app:layout_constraintEnd_toEndOf="@id/guidelineDialogMenu2"
            app:layout_constraintStart_toStartOf="@id/guidelineDialogMenu1"
            app:layout_constraintTop_toBottomOf="@id/btnPaqueteDialog" />

        <View
            android:layout_width="0dp"
            android:layout_height="1dp"
            android:background="@android:color/darker_gray"
            app:layout_constraintTop_toBottomOf="@id/btnHorariosDialog"
            app:layout_constraintEnd_toEndOf="@id/guidelineDialogMenu2"
            app:layout_constraintStart_toStartOf="@id/guidelineDialogMenu1"/>



        <com.google.android.material.button.MaterialButton
            android:id="@+id/btnSalonesDialog"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:backgroundTint="@color/white"
            android:textAllCaps="false"
            android:text="Tipos de clase"
            android:textAlignment="viewStart"
            android:textColor="@color/black"
            app:strokeWidth="0dp"
            app:icon="@drawable/salones"
            app:iconTint="@color/black"
            android:elevation="0dp"
            style="@style/Widget.AppCompat.Button.Borderless"
            app:layout_constraintEnd_toEndOf="@id/guidelineDialogMenu2"
            app:layout_constraintStart_toStartOf="@id/guidelineDialogMenu1"
            app:layout_constraintTop_toBottomOf="@id/btnNuestrasClases" />

        <View
            android:layout_width="0dp"
            android:layout_height="1dp"
            android:background="@android:color/darker_gray"
            app:layout_constraintTop_toBottomOf="@id/btnSalonesDialog"
            app:layout_constraintEnd_toEndOf="@id/guidelineDialogMenu2"
            app:layout_constraintStart_toStartOf="@id/guidelineDialogMenu1"/>

        <com.google.android.material.button.MaterialButton
            android:id="@+id/btnUbícanosDialog"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:backgroundTint="@color/white"
            android:text="Ubícanos"
            android:textAllCaps="false"
            android:textAlignment="viewStart"
            android:textColor="@color/black"
            app:strokeWidth="0dp"
            app:icon="@drawable/ubicanos"
            app:iconTint="@color/black"
            android:elevation="0dp"
            style="@style/Widget.AppCompat.Button.Borderless"
            app:layout_constraintEnd_toEndOf="@id/guidelineDialogMenu2"
            app:layout_constraintStart_toStartOf="@id/guidelineDialogMenu1"
            app:layout_constraintTop_toBottomOf="@id/btnSalonesDialog" />

        <View
            android:layout_width="0dp"
            android:layout_height="1dp"
            android:background="@android:color/darker_gray"
            app:layout_constraintTop_toBottomOf="@id/btnUbícanosDialog"
            app:layout_constraintEnd_toEndOf="@id/guidelineDialogMenu2"
            app:layout_constraintStart_toStartOf="@id/guidelineDialogMenu1"/>

        <com.google.android.material.button.MaterialButton
            android:id="@+id/btnPaqueteDialog"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:backgroundTint="@color/white"
            android:text="Ver Paquete"
            android:textAllCaps="false"
            android:textAlignment="viewStart"
            android:textColor="@color/black"
            app:strokeWidth="0dp"
            app:icon="@drawable/verpaquete"
            app:iconTint="@color/black"
            android:elevation="0dp"
            style="@style/Widget.AppCompat.Button.Borderless"
            app:layout_constraintEnd_toEndOf="@id/guidelineDialogMenu2"
            app:layout_constraintStart_toStartOf="@id/guidelineDialogMenu1"
            app:layout_constraintTop_toBottomOf="@id/btnConocenosDialog" />

        <View
            android:layout_width="0dp"
            android:layout_height="1dp"
            android:background="@android:color/darker_gray"
            app:layout_constraintTop_toBottomOf="@id/btnPaqueteDialog"
            app:layout_constraintEnd_toEndOf="@id/guidelineDialogMenu2"
            app:layout_constraintStart_toStartOf="@id/guidelineDialogMenu1"/>

        <com.google.android.material.button.MaterialButton
            android:id="@+id/btnConocenosDialog"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:backgroundTint="@color/white"
            android:text="Conocenos"
            android:textAllCaps="false"
            android:textAlignment="viewStart"
            android:textColor="@color/black"
            app:strokeWidth="0dp"
            app:icon="@drawable/conocenos_icono"
            app:iconTint="@color/black"
            android:elevation="0dp"
            style="@style/Widget.AppCompat.Button.Borderless"
            app:layout_constraintEnd_toEndOf="@id/guidelineDialogMenu2"
            app:layout_constraintStart_toStartOf="@id/guidelineDialogMenu1"
            app:layout_constraintTop_toBottomOf="@id/btnPerfil" />

        <com.google.android.material.button.MaterialButton
            android:id="@+id/btnPerfil"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:backgroundTint="@color/white"
            android:text="Perfil"
            android:textAllCaps="false"
            android:textAlignment="viewStart"
            android:textColor="@color/black"
            app:strokeWidth="0dp"
            app:icon="@drawable/iconop"
            app:iconTint="@color/black"
            android:elevation="0dp"
            style="@style/Widget.AppCompat.Button.Borderless"
            app:layout_constraintEnd_toEndOf="@id/guidelineDialogMenu2"
            app:layout_constraintStart_toStartOf="@id/guidelineDialogMenu1"
            app:layout_constraintTop_toTopOf="parent" />

        <View
            android:layout_width="0dp"
            android:layout_height="1dp"
            android:background="@android:color/darker_gray"
            app:layout_constraintTop_toBottomOf="@id/btnPerfil"
            app:layout_constraintEnd_toEndOf="@id/guidelineDialogMenu2"
            app:layout_constraintStart_toStartOf="@id/guidelineDialogMenu1"/>

        <View
            android:layout_width="0dp"
            android:layout_height="1dp"
            android:background="@android:color/darker_gray"
            app:layout_constraintTop_toBottomOf="@id/btnConocenosDialog"
            app:layout_constraintEnd_toEndOf="@id/guidelineDialogMenu2"
            app:layout_constraintStart_toStartOf="@id/guidelineDialogMenu1"/>

        <View
            android:layout_width="0dp"
            android:layout_height="1dp"
            android:background="@android:color/darker_gray"
            app:layout_constraintTop_toBottomOf="@id/btnNuestrasClases"
            app:layout_constraintEnd_toEndOf="@id/guidelineDialogMenu2"
            app:layout_constraintStart_toStartOf="@id/guidelineDialogMenu1"/>




        <com.google.android.material.button.MaterialButton
            android:id="@+id/btnLogoutDialog"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:backgroundTint="@color/white"
            android:text="Logout"
            android:textAllCaps="false"
            android:textAlignment="viewStart"
            android:textColor="@color/black"
            app:strokeWidth="0dp"
            app:icon="@drawable/logout"
            app:iconTint="@color/black"
            android:elevation="0dp"
            style="@style/Widget.AppCompat.Button.Borderless"
            app:layout_constraintBottom_toTopOf="@id/eliminarCuenta"
            app:layout_constraintEnd_toEndOf="@id/guidelineDialogMenu2"
            app:layout_constraintStart_toStartOf="@id/guidelineDialogMenu1"
            app:layout_constraintTop_toBottomOf="@id/btnGaleria" />

        <com.google.android.material.button.MaterialButton
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/btnNuestrasClases"
            android:textAllCaps="false"
            app:strokeWidth="0dp"
            android:textAlignment="viewStart"
            android:textColor="@color/black"
            app:icon="@drawable/nuestras_clases_icono"
            app:iconTint="@color/black"
            android:text="Nuestras Clases"
            style="@style/Widget.AppCompat.Button.Borderless"
            app:layout_constraintStart_toStartOf="@+id/guidelineDialogMenu1"
            app:layout_constraintEnd_toEndOf="@+id/guidelineDialogMenu2"
            app:layout_constraintTop_toBottomOf="@+id/btnHorariosDialog"/>

        <View
            android:layout_width="0dp"
            android:layout_height="1dp"
            android:background="@android:color/darker_gray"
            app:layout_constraintTop_toBottomOf="@id/btnGaleria"
            app:layout_constraintEnd_toEndOf="@id/guidelineDialogMenu2"
            app:layout_constraintStart_toStartOf="@id/guidelineDialogMenu1"/>

        <com.google.android.material.button.MaterialButton
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/btnGaleria"
            android:textAllCaps="false"
            app:strokeWidth="0dp"
            android:textAlignment="viewStart"
            android:textColor="@color/black"
            app:icon="@drawable/galeria_icono"
            app:iconTint="@color/black"
            android:text="@string/Galeria"
            style="@style/Widget.AppCompat.Button.Borderless"
            app:layout_constraintStart_toStartOf="@+id/guidelineDialogMenu1"
            app:layout_constraintEnd_toEndOf="@+id/guidelineDialogMenu2"
            app:layout_constraintTop_toBottomOf="@+id/btnUbícanosDialog"
            app:layout_constraintBottom_toTopOf="@id/btnLogoutDialog"/>

        <View
            android:layout_width="0dp"
            android:layout_height="1dp"
            android:background="@android:color/darker_gray"
            app:layout_constraintTop_toBottomOf="@id/btnLogoutDialog"
            app:layout_constraintEnd_toEndOf="@id/guidelineDialogMenu2"
            app:layout_constraintStart_toStartOf="@id/guidelineDialogMenu1"/>

        <com.google.android.material.button.MaterialButton
            android:id="@+id/eliminarCuenta"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:backgroundTint="@color/white"
            android:text="Eliminar Cuenta"
            android:textAllCaps="false"
            android:textAlignment="viewStart"
            android:textColor="@color/black"
            app:strokeWidth="0dp"
            app:icon="@drawable/eliminarcuenta"
            app:iconTint="@color/black"
            android:elevation="0dp"
            style="@style/Widget.AppCompat.Button.Borderless"
            android:layout_marginBottom="15dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@id/guidelineDialogMenu2"
            app:layout_constraintStart_toStartOf="@id/guidelineDialogMenu1"
            app:layout_constraintTop_toBottomOf="@id/btnLogoutDialog" />



        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guidelineDialogMenu1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.10" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guidelineDialogMenu2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.90" />


    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>